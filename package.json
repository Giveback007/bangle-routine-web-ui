		"@tailwindcss/vite": "^4.1.17",
		"pwa-asset-generator": "^8.1.2",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.2",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"@giveback007/util-lib": "3.3.1"
	},
